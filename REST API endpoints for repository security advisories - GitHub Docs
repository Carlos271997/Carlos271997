input.FormControl-checkbox[type="checkbox"]:not([type="checkbox"], [type="radio"]):focus-visible { border-color: var(--focus-outlineColor,var(--color-accent-fg)); box-shadow: inset 0 0 0 1px var(--focus-outlineColor,var(--color-accent-fg)); outline: none; }

