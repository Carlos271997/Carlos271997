  <span class="hljs-attr">"html_url"</span><span class="hljs-punctuation">:</span> <span class="hljs-string">"https://github.com/repo/a-package/security/advisories/GHSA-abcd-1234-efgh"</span><span class="hljs-punctuation">,</span>
